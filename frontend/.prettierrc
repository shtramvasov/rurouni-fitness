{
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"semi": false,
	"trailingComma": "all",
	"importOrder": [
		"^@core/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^@pages/(.*)$",
		"^@components/(.*)$",
		"^@utils/(.*)$",
		"^@services/(.*)$",
		"^@store/(.*)$",
		"^@hooks/(.*)$",
		"^[./]",
		"^@assets/(.*)$"
	]
}